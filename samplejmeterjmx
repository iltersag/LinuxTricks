          <stringProp name="BeanShellSampler.query">try{
	BufferedReader reader = new BufferedReader(new FileReader(&quot;/home/path/jsonreq.json&quot;));


int counter = 0;        

for(String line; (line = reader.readLine()) != null; ) {
    vars.put(&quot;csv_line_&quot; + String.valueOf(counter), line);
    log.info(&quot;test.txt file content is: &quot; + vars.get(&quot;csv_line_&quot; + counter));
    counter++;
}  
}
catch (Throwable ex) {
    log.error(&quot;Error in Beanshell&quot;, ex);
    throw ex;
}
</stringProp>
